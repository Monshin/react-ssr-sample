{
  "name": "react-ssr-sample",
  "version": "1.0.0",
  "description": "React Server side render by Express, including Material-ui, react-helmet-async, Redux, Redux-Observable, Redux Form, React-Router",
  "main": "dist/server.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "debug": "npm run build:debug && nodemon ./dist/server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "build:debug": "cross-env NODE_ENV=debug webpack --progress --config webpack.dev.js",
    "compile:config": "tsc ./src/config/config --lib es2016,dom",
    "clear": "rm -rf ./dist & rm -rf ./build"
  },
